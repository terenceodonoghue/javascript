# Vite base path — default '/' for dev; set to '/login/' in production build (Dockerfile default)
VITE_BASE=/

# Auth API prefix — default '' for dev (Vite proxies /api to localhost:8081); set to '/auth' in production build (Dockerfile default)
VITE_API_BASE=

# App title displayed on the login page — default 'Homelab' in production build (Dockerfile default)
VITE_APP_TITLE=Homelab
